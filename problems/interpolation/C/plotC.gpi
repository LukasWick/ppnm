set term svg font "Arial,20" size 1024,1024 background rgb "white"
set out "plotC.svg"
set xlabel "x"
set ylabel "y"
set key top left
set xzeroaxis
set yzeroaxis
set title "Cspline interpolation"
set multiplot layout 2,2
set title "Spline"
plot "out-cSpline.txt" with lines title "spline"\
,"out-xydata.txt" title "cos(x)"
#
set title "Intergral"
plot "out-cIntegral.txt" using ($1):($2) with lines title "int(cos(x))"\
,"out-cIntegral.txt" using ($1):($3) with lines title "sin(x)"
#
set title "Derivative"
plot "out-cDerivative.txt" using ($1):($2) with lines title "cos'(x)"\
,"out-cDerivative.txt" using ($1):($3) with lines title "-sin(x)"
#
unset multiplot