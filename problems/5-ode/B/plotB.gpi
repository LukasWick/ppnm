set term svg font "Arial,20" size 1024,600 background rgb "white"
set out "plotB.svg"
set xlabel "t (days)"
set ylabel "Number of people"
set title "SIR model of COVID-19 in denmark"
set key top right
plot "out.plotB.txt" using ($1):($2) with lines title "S"\
, "out.plotB.txt" using ($1):($3) with lines title "I"\
,"out.plotB.txt" using ($1):($4) with lines title "R"

