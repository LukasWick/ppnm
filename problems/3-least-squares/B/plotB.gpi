set term svg font "Arial,20" size 1024,600 background rgb "white"
set out "plotB.svg"
set xlabel "t"
set ylabel "y"
set rmargin 0
set title "Least square fit"
plot "out-tydata.txt" using ($1):($2):($3) with yerrorbars title "data"\
,"out.plot.txt" using ($1):($2) with lines title "fit"\
,"out.plot.txt" using ($1):($2+$3) with lines title ""\
,"out.plot.txt" using ($1):($2-$3) with lines title ""

